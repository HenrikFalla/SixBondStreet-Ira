{% assign nosto_settings = 'frontpage-nosto-' | append: section.settings.nosto %}
{{ nosto_settings }}
{% render 'nosto-element', id: 'frontpage-nosto-{{ settings.nosto_section.nosto-id}}'%}
{% schema %}
    {
        "name": "Nosto",
        "class": "nosto-section",
        "settings": [
            {
                "id": "nosto",
                "type": "range",
                "label": "Select which nosto section to display",
                "info": "Numbers 1 through 4 reference spesific sections set up in the nosto admin panel",
                "min": 1,
                "max": 4,
                "step": 1,
                "default": 1
            }
        ],
        "presets": [
            {
                "name": "Nosto",
                "category": "Product"
            }
        ]
    }
{% endschema %}